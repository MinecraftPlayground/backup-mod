{
	"schemaVersion": 1,
	"id": "backup",
	"version": "${version}",
	"name": "backup",
	"description": "This Minecraft Fabric Mod provides an easy way of saving and restoring backups",
	"authors": [
		"Mqx"
	],
	"contact": {
		"homepage": "https://github.com/MinecraftPlayground/backup-mod",
		"sources": "https://github.com/MinecraftPlayground/backup-mod"
	},
	"license": "LICENSE",
	"icon": "assets/backup/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.loat.Backup"
		],
		"client": [
			"dev.loat.BackupClient"
		]
	},
	"mixins": [
		"backup.mixins.json",
		{
			"config": "backup.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
